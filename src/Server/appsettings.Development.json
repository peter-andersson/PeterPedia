{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Quartz": {
    "RemoveArticleJob": "0 0 0 * * ?",
    "RefreshArticleJob": "0 */5 * * * ?",
    "VideoJob": "0 */10 * * * ?",
    "ShowUpdateJob": "0 */5 * * * ?",
    "PhotoJob": "0 0/5 * * * ?",
    "MovieUpdateJob": "0 */1 * * * ?"
  },
  "ConnectionStrings": {
    "PeterPedia": "Data source=peterpedia.db"
  }  
}
